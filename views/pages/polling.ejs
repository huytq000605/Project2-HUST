<script>
  const polling = () => {
    fetch("http://localhost:3000/polling", {method: "GET"}).then((response) => {
      response.json().then(data => {
        console.log(data)
        setTimeout(polling, 5000)
      }).catch(err => {
        console.log(err)
        setTimeout(polling, 5000)
      })
    })
  }
  setTimeout(polling, 5000)
</script>
